<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Toolkit</id>
    <version>1.0.4</version>
    <authors>Távio Vieira Aráujo</authors>
    <license type="expression">MIT</license>
    <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
    <description>Toolkit para facilitar o uso do Kafka em .NET</description>
    <repository type="git" url="https://github.com/taviovieira89/ToolKit" commit="e6ca4363c46afdc7e8d4f7762a82f26fe96c6b70" />
    <dependencies>
      <group targetFramework="net9.0">
        <dependency id="Confluent.Kafka" version="2.8.0" exclude="Build,Analyzers" />
        <dependency id="MediatR" version="12.4.1" exclude="Build,Analyzers" />
        <dependency id="MediatR.Extensions.Microsoft.DependencyInjection" version="11.1.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.EntityFrameworkCore" version="9.0.1" exclude="Build,Analyzers" />
        <dependency id="Microsoft.EntityFrameworkCore.SqlServer" version="9.0.1" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.Hosting" version="9.0.1" exclude="Build,Analyzers" />
        <dependency id="MongoDB.Driver" version="3.1.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <!-- Caminhos relativos para os arquivos -->
    <file src="ExampleToolKit.dll" target="lib/net9.0/ExampleToolKit.dll" />
    <file src="ExampleToolKit.runtimeconfig.json" target="lib/net9.0/ExampleToolKit.runtimeconfig.json" />
  </files>
</package>